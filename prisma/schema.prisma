// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Event {
  id          String   @id
  dateFetched DateTime @default(now())
  title       String   @unique
  link        String
  content     String
  pubDate     DateTime
  creator     String
}
